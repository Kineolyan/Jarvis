{
  "name": "jarvis",
  "version": "1.0.0",
  "description": "My own Jarvis project",
  "main": "bin/summer",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "clean": "del-cli ./dist",
    "do_the_thing": "npm run clean && npm run build && npm test",
    "test": "jest",
    "tsc": "tsc",
    "watch:build": "tsc --project ./tsconfig.json --watch",
    "watch:test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kineolyan/Jarvis.git"
  },
  "keywords": [
    "jarvis"
  ],
  "author": "Kineolyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kineolyan/Jarvis/issues"
  },
  "homepage": "https://github.com/Kineolyan/Jarvis#readme",
  "dependencies": {
    "chai": "4.2.0",
    "chokidar": "2.0.4",
    "del-cli": "1.1.0",
    "lodash": "4.17.11",
    "mocha": "5.2.0",
    "rxjs": "6.3.3",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chokidar": "1.7.5",
    "@types/jest": "23.3.10",
    "@types/lodash": "4.14.119",
    "@types/node": "10.12.15",
    "jest": "23.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/spec/preprocessor.js"
    },
    "testRegex": ".*\\.spec\\.(ts|tsx|js)$"
  }
}
