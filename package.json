{
  "name": "jarvis",
  "version": "1.0.0",
  "description": "My own Jarvis project",
  "main": "bin/summer",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "clean": "del-cli ./dist",
    "do_the_thing": "npm run clean && npm run build && npm test",
    "test": "jest",
    "tsc": "tsc",
    "watch:build": "tsc --project ./tsconfig.json --watch",
    "watch:test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kineolyan/Jarvis.git"
  },
  "keywords": [
    "jarvis"
  ],
  "author": "Kineolyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kineolyan/Jarvis/issues"
  },
  "homepage": "https://github.com/Kineolyan/Jarvis#readme",
  "dependencies": {
    "chai": "^3.5.0",
    "chokidar": "^1.6.1",
    "del-cli": "^0.2.0",
    "lodash": "^4.11.2",
    "mocha": "^3.2.0",
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chokidar": "^1.4.31",
    "@types/jest": "^18.1.1",
    "@types/lodash": "^4.14.54",
    "@types/node": "^7.0.5",
    "jest": "^19.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/spec/preprocessor.js"
    },
    "testRegex": ".*\\.spec\\.(ts|tsx|js)$"
  }
}
